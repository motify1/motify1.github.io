{
  "manifest_version": 3,
  "name": "My Website Extension",
  "description": "An extension that loads my website.",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "icon.jpg",
      "48": "icon.jpg",
      "128": "icon.jpg"
