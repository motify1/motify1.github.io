{
  "name": "focus-tree",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "homepage": "https://motify1.github.io/",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web_ssl": "SET NODE_OPTIONS=--openssl-legacy-provider && expo start --web ",
    "deploy": "gh-pages -t -d dist",
    "predeploy": "expo export --platform web & yarn create-workout-greeting-screen & yarn create-workout-user-setting-screen & yarn create-workout-profile-screen & yarn create-workout-mission-screen & yarn create-workout-mission-detail-screen",
    "predeploy2": "expo export --platform web & yarn create-focus-farm-screen & yarn create-farm-collection-screen & yarn create-wishes-list-screen & yarn create-audio & yarn create-wish-detail-screen & yarn create-fishing-farm-screen",
    "create-focus-farm-screen": "mkdir dist\\focus-farm-screen & copy dist\\index.html dist\\focus-farm-screen\\index.html",
    "create-fishing-farm-screen": "mkdir dist\\fishing-farm-screen & copy dist\\index.html dist\\fishing-farm-screen\\index.html",
    "create-farm-collection-screen": "mkdir dist\\farm-collection-screen & copy dist\\index.html dist\\farm-collection-screen\\index.html",
    "create-wishes-list-screen": "mkdir dist\\wishes-list-screen & copy dist\\index.html dist\\wishes-list-screen\\index.html",
    "create-wish-detail-screen": "mkdir dist\\wish-detail-screen & copy dist\\index.html dist\\wish-detail-screen\\index.html",
    "create-workout-greeting-screen": "mkdir dist\\workout-greeting-screen & copy dist\\index.html dist\\workout-greeting-screen\\index.html",
    "create-workout-user-setting-screen": "mkdir dist\\workout-user-setting-screen & copy dist\\index.html dist\\workout-user-setting-screen\\index.html",
    "create-workout-profile-screen": "mkdir dist\\workout-profile-screen & copy dist\\index.html dist\\workout-profile-screen\\index.html",
    "create-workout-mission-screen": "mkdir dist\\workout-mission-screen & copy dist\\index.html dist\\workout-mission-screen\\index.html",
    "create-workout-mission-detail-screen": "mkdir dist\\workout-mission-detail-screen & copy dist\\index.html dist\\workout-mission-detail-screen\\index.html",
    "create-audio": "mkdir dist\\assets\\audio && xcopy assets\\audio dist\\assets\\audio",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.0",
    "@expo/webpack-config": "^19.0.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "^8.0.1",
    "@react-native-material/core": "^1.3.7",
    "@react-navigation/native": "~7.1.6",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.6.8",
    "babel-plugin-module-resolver": "^5.0.0",
    "dayjs": "^1.11.7",
    "expo": "~53.0.0",
    "expo-auth-session": "~6.1.5",
    "expo-av": "~15.1.4",
    "expo-background-fetch": "~13.0.6",
    "expo-checkbox": "~4.1.4",
    "expo-clipboard": "~7.1.4",
    "expo-crypto": "~14.1.4",
    "expo-device": "~7.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-location": "~18.1.4",
    "expo-notifications": "~0.31.1",
    "expo-router": "~5.0.5",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.1.6",
    "jest": "^29.6.2",
    "jest-expo": "~52.0.6",
    "kill-port": "^2.0.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-actions-sheet": "^0.8.21",
    "react-native-calendars": "^1.1302.0",
    "react-native-element-dropdown": "^2.9.0",
    "react-native-logs": "^5.0.1",
    "react-native-popup-menu": "^0.16.1",
    "react-native-progress": "^5.0.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-swiper": "^1.6.0",
    "react-native-uuid": "^2.0.1",
    "react-native-web": "^0.20.0",
    "react-redux": "^8.0.5",
    "typescript": "~5.8.3",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^0.15.0",
    "@aws-amplify/backend-cli": "^0.15.0",
    "@aws-amplify/core": "^6.3.1",
    "@aws-amplify/graphql-api-construct": "^1.9.0",
    "@aws-amplify/graphql-transformer-core": "^2.7.0",
    "@aws-amplify/react-native": "^1.1.1",
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.9.0",
    "@testing-library/react-native": "^12.2.2",
    "aws-amplify": "^6.3.4",
    "babel-jest": "29.6.2",
    "eslint": "^9.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-transforms": "^2.0.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFiles": [
      "./jestSetup.js"
    ],
    "transform": {
      ".*(ts|tsx|js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "aws-amplify": "<rootDir>/__tests__/unit-tests/mocks/aws-amplify.js"
    },
    "collectCoverage": true
  },
  "private": true
}
